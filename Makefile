NAME = push_swap
PRINT_NAME		= push-swap
PRINT_PREFIX	= \033[1m\033[38;5;240m[\033[0m\033[38;5;250m$(PRINT_NAME)\033[1m\033[38;5;240m] \033[38;5;105m~\033[0m

SRC = 		push-swap \
			parsing \
			parsing_2 \
			parsing_init \
			actions \
			actions_doubled \
			utils \
			utils2 \
			sort_small_a \
			sort_big \

BONUSRC = 	src/checker/checker.c \
			src/checker/get_next_line_L.c \
			src/checker/get_next_line_utils_L.c \
			src/actions.c \
			src/actions_doubled.c \
			src/utils.c \
			src/parsing.c \
			src/parsing_2.c \
			src/parsing_init.c \

#OBJECTS = $(SRC:.c=.o)

#INCLUDES    = -I includes -I $(LIBFT_DIR)/includes/

CC = gcc
CFLAGS = -Wall -Werror -Wextra -g3

LIBFT_DIR	= inc/libft/
DIR_SRCS    = src/
DIR_OBJS    = objs/

SRCS        = $(addprefix $(DIR_SRCS), $(addsuffix .c, $(SRC)))
OBJS        = $(addprefix $(DIR_OBJS), $(addsuffix .o, $(SRC)))
OBJSBONUS	= $(BONUSRC:%.c=%.o)

all: $(NAME)
#			@$(ECHO) All done

$(NAME): $(OBJS)
		@echo "$(PRINT_PREFIX)\033[0;38;5;226m Compiling \033[0m\n"
		@make -C inc/libft
		@make -C inc/libft bonus
		@$(CC) $(CFLAGS) -o $(NAME) $(OBJS) $(LIBFT_DIR)libft.a
		@echo "$(PRINT_PREFIX)\033[0;38;5;226m Done \033[0m\n"

bonus: $(OBJSBONUS)
		@make -C inc/libft
		@$(CC) $(CFLAGS) -o checker $(OBJSBONUS) $(LIBFT_DIR)libft.a

$(DIR_OBJS)%.o :	$(DIR_SRCS)%.c $(OBJF)
				@mkdir -p $(DIR_OBJS)
				@$(CC) $(CFLAGS) -c $< -o $@
#$(LIBFT):
#			@make -C $(LIBFT_DIR)

clean:
		@echo "$(PRINT_PREFIX)\033[0;38;5;226m Cleaning \033[0m\n"
		@rm -f $(OBJS)
		@rm -f $(OBJSBONUS)
		@make -C inc/libft/ clean
		@rm -r $(DIR_OBJS)
		@echo "$(PRINT_PREFIX)\033[0;38;5;226m Done \033[0m\n"

fclean: clean
		@rm -f $(NAME)

re: fclean all

.PHONY: all clean fclean re test

